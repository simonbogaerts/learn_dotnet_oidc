@model ImageGallery.Client.ViewModels.OrderFrameViewModel

@{
    ViewData["Title"] = "OrderFrame";
}

<h2>OrderFrame</h2>
<div class="container">
    <div class="h3 bottomMarginDefault">Order a framed version of your favourite picture.</div>

    <div class="text-capitalize bottomMarginSmall">We've got this address on record for you: </div>

    <div class="text-capitalize text-info bottomMarginSmall">@Model.Address</div>

    <div class="text">If this is correct, please contact us.</div>
</div>
